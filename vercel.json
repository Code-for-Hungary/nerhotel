{
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "trailingSlash": false,
    "headers": [
        {
            "source": "/nh-cover.png",
            "headers": [
                {
                    "key": "Content-Encoding",
                    "value": "gzip, deflate"
                }
            ]
        }
    ]
}
